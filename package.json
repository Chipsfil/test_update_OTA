{
  "name": "electron-ota-test",
  "version": "2.0.0",
  "description": "Electron application for testing OTA updates",
  "main": "main.js",
  "author": "Test Author",
  "license": "MIT",
  "homepage": "https://github.com/Chipsfil/test_update_OTA.git",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "publish": "electron-builder --publish always"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "26.8.1"
  },
  "dependencies": {
    "electron-updater": "6.8.3"
  },
  "build": {
    "appId": "com.electron.ota.test",
    "productName": "Electron OTA Test",
    "files": [
      "main.js",
      "preload.js",
      "renderer.js",
      "index.html",
      "page2.html",
      "style.css",
      "assets/**/*",
      "fonts/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "publish": {
      "provider": "github",
      "owner": "Chipsfil",
      "repo": "test_update_OTA"
    }
  }
}
